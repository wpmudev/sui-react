  "input": "./src",
  "output": "./dist",
  "directories": {
    "src": "src"
  },
  "files": [
    "dist"
  ],
  "style": "dist/css/sui-css.min.css",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wpmudev/sui-css.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "tokens": "style-dictionary build",
    "build": "npm run tokens && gulp",
    "prepare": "npm run clean && gulp",
    "prepack": "clean-package",
    "postpack": "clean-package restore",
    "publish:test": "npm run prepare && npm version prerelease --git-tag-version=false && npm pack",
    "publish:patch": "npm run prepare && npm version patch -m \"üì¶Ô∏è SUI CSS Framework %s\" && npm publish",
    "publish:minor": "npm run prepare && npm version minor -m \"üì¶Ô∏è SUI CSS Framework %s\" && npm publish",
    "publish:major": "npm run prepare && npm version major -m \"üì¶Ô∏è SUI CSS Framework %s\" && npm publish"
  },
  "bugs": {
    "url": "https://github.com/wpmudev/sui-css"
  },
  "homepage": "https://github.com/wpmudev/sui-css#readme",
  "clean-package": "./postpack.config.json"
}
